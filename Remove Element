class Solution {
public:
    int removeElement(vector<int>& arr, int x) {
        int count = 0;
        int n = arr.size();
        for(int i=0; i<n; i++){
            if(arr[i] != x ){
                arr[count] = arr[i];
                count++;
            }            
        }
    return count;
    }
};

